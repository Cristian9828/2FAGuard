<Page
    x:Class="Guard.Views.Pages.TokenSuccessPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Guard.Views.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    Title="SingleTokenPage"
    d:Background="White"
    d:DesignHeight="700"
    d:DesignWidth="1150"
    mc:Ignorable="d">

    <Grid>
        <StackPanel x:Name="TokenCardContainer" HorizontalAlignment="Center">
            <svgc:SvgViewbox
                Width="200"
                Margin="0,45,0,40"
                HorizontalAlignment="Center"
                Source="pack://application:,,,/Assets/check.svg" />
            <ui:Button
                x:Name="HomeButton"
                Margin="0,40,0,0"
                HorizontalAlignment="Center"
                Appearance="Primary"
                Click="HomeButton_Click"
                Content="{DynamicResource i.stp.home}"
                Icon="{ui:SymbolIcon Key24,
                                     Filled=True}" />
        </StackPanel>
    </Grid>
</Page>
